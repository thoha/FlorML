<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:mods="http://www.loc.gov/mods/v3" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://www.loc.gov/mods/v3" schemaLocation="http://www.loc.gov/standards/mods/v3/mods-3-1.xsd"/>
	<xs:element name="publication">
		<xs:annotation>
			<xs:documentation>publication, e.g. flora, fauna, taxonomic article</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="metaData"/>
				<xs:element name="textSection" type="textSectionType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>for any text that is not part of a taxonomic treatment or addenda/errata/corrigenda, and not metadata</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="treatment" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>a taxonomic treatment</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="addenda" type="addendaType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>addenda, errata, corrigenda</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="lang" type="xs:language">
				<xs:annotation>
					<xs:documentation>language of publication</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="defaultMediaUrl">
		<xs:annotation>
			<xs:documentation>base URL for illustrations</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="defaultMeasurementSystem">
		<xs:annotation>
			<xs:documentation>SI, Imperial, etc.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="metaData">
		<xs:annotation>
			<xs:documentation>metadata</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="metaDataType">
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="defaultMediaUrl"/>
						<xs:element ref="defaultMeasurementSystem" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Use to define a default measurement system as used throughout the publication. SI, Imperial, etc.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:any namespace="http://www.loc.gov/mods/v3"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="acceptedName">
		<xs:annotation>
			<xs:documentation>accepted name</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="nom" type="nomType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumPublication">
		<xs:annotation>
			<xs:documentation>to what publication is this an addendum?</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="refPart" type="refPartType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>reference parts</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumTaxon">
		<xs:annotation>
			<xs:documentation>to what taxon is this an addendum?</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="name" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumOther">
		<xs:annotation>
			<xs:documentation>to what else is this an addendum? only use if addendum doesn't contain taxonomic information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeCollectionTypeStatus">
		<xs:annotation>
			<xs:documentation>alternative collection type status</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeCollector">
		<xs:annotation>
			<xs:documentation>alternative collector</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeFieldNum">
		<xs:annotation>
			<xs:documentation>alternative number</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
				<xs:element name="typeNotes" type="notesType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Notes on the type or the type status. DON'T put nomenclatural notes in here!</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="doubtful" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="altitude">
		<xs:annotation>
			<xs:documentation>altitude</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="estimate" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>if altitude is explicitly indicated to be estimate, set to true.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="range" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>set to true if instead of a single value a range is given</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="value">
		<xs:annotation>
			<xs:documentation>for numerical values in altitudes, characters</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="class">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="mininum"/>
						<xs:enumeration value="average"/>
						<xs:enumeration value="median"/>
						<xs:enumeration value="maximum"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="b">
		<xs:annotation>
			<xs:documentation>text in bold</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="br">
		<xs:annotation>
			<xs:documentation>line break</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="vernacularNames" type="vernacularNamesType">
		<xs:annotation>
			<xs:documentation>vernacular names</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="vernacularName">
		<xs:annotation>
			<xs:documentation>one vernacular name</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element name="refPart" type="refPartType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="name" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>in this location always use with class="vernacular", one set of tags for each vernacular name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="localLanguage" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>local language (for vernacular names). One set of tags per local language.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="translation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="locality" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>comment by treatment author</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="localLanguage">
		<xs:annotation>
			<xs:documentation>local language (for vernacular names)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="doubtful" type="xs:boolean"/>
			<xs:attribute name="unknown" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="translation" type="xs:string">
		<xs:annotation>
			<xs:documentation>translation of a vernacular name into the base language of a taxonomic work</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="charClass">
		<xs:annotation>
			<xs:documentation>for any character classes not included in elements char and subChar</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="charPart">
		<xs:annotation>
			<xs:documentation>child character part(s) (freetext field)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>annotation by the treatment's author</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="footnote" type="footnoteType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="class">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="definition"/>
						<xs:enumeration value="state"/>
						<xs:enumeration value="value"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="doubtful" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="childChar" type="childCharType">
		<xs:annotation>
			<xs:documentation>child character element for further atomisation of (sub)characters</xs:documentation>
		</xs:annotation>
	</xs:element>

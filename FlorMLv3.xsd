<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="publication">
		<xs:annotation>
			<xs:documentation>publication</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="metaData"/>
				<xs:element ref="biographies" minOccurs="0"/>
				<xs:element ref="references" minOccurs="0"/>
				<xs:element ref="textSection" minOccurs="0"/>
				<xs:element ref="treatment"/>
				<xs:element ref="addenda" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="metaData">
		<xs:annotation>
			<xs:documentation>metadata</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="metaDataType">
					<xs:choice maxOccurs="unbounded">
						<xs:any namespace="http://www.loc.gov/standards/mods/v3/mods-3-3.xsd" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="defaultMediaUrl"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="defaultMediaUrl">
		<xs:annotation>
			<xs:documentation>base URL for illustrations</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="treatment">
		<xs:annotation>
			<xs:documentation>taxonomic treatment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="taxon" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="textSection" type="textSectionType">
		<xs:annotation>
			<xs:documentation>Text sections not covered by treatment, bibliography, biographies, or addenda/etc.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="biographies" type="biographiesType">
		<xs:annotation>
			<xs:documentation>biographies</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="table" type="tableType">
		<xs:annotation>
			<xs:documentation>table</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tr">
		<xs:annotation>
			<xs:documentation>table row</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="th" minOccurs="0"/>
				<xs:element ref="td" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="th" type="thType">
		<xs:annotation>
			<xs:documentation>table header cell</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="td" type="tableCellType">
		<xs:annotation>
			<xs:documentation>table cell</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ol" type="orderedListType">
		<xs:annotation>
			<xs:documentation>ordered list</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ul" type="unorderedListType">
		<xs:annotation>
			<xs:documentation>unordered list</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="li" type="listItemType">
		<xs:annotation>
			<xs:documentation>list item</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="br">
		<xs:annotation>
			<xs:documentation>line break</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="superscript">
		<xs:annotation>
			<xs:documentation>superscript</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="subscript">
		<xs:annotation>
			<xs:documentation>subscript</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bold">
		<xs:annotation>
			<xs:documentation>text in bold</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="italics">
		<xs:annotation>
			<xs:documentation>text in italics</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="heading" type="headingType">
		<xs:annotation>
			<xs:documentation>heading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="subHeading" type="subheadingType">
		<xs:annotation>
			<xs:documentation>subheading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="annotation" type="annotationType">
		<xs:annotation>
			<xs:documentation>annotations</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="string" type="stringType">
		<xs:annotation>
			<xs:documentation>text string</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="taxontype">
		<xs:annotation>
			<xs:documentation>For types mentioned outside of nomenclature</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="feature">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0"/>
				<xs:element ref="writer" minOccurs="0"/>
				<xs:element ref="distributionList" minOccurs="0"/>
				<xs:element ref="habitatList" minOccurs="0"/>
				<xs:element ref="char" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="string" minOccurs="0"/>
				<xs:element ref="figure" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0"/>
			</xs:choice>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="anatomy"/>
						<xs:enumeration value="biogeography"/>
						<xs:enumeration value="conservation"/>
						<xs:enumeration value="cultivation"/>
						<xs:enumeration value="cytology"/>
						<xs:enumeration value="description"/>
						<xs:enumeration value="dispersal"/>
						<xs:enumeration value="distribution"/>
						<xs:enumeration value="ecology"/>
						<xs:enumeration value="embryology"/>
						<xs:enumeration value="fossils"/>
						<xs:enumeration value="habitat"/>
						<xs:enumeration value="habitatecology"/>
						<xs:enumeration value="history"/>
						<xs:enumeration value="introducedspecies"/>
						<xs:enumeration value="lifehistory"/>
						<xs:enumeration value="morphology"/>
						<xs:enumeration value="palynology"/>
						<xs:enumeration value="parasitism"/>
						<xs:enumeration value="phytochemo"/>
						<xs:enumeration value="phylogeny"/>
						<xs:enumeration value="pollination"/>
						<xs:enumeration value="taxonomy"/>
						<xs:enumeration value="uses"/>
						<xs:enumeration value="vernacular"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="writer" type="writerType">
		<xs:annotation>
			<xs:documentation>writer</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="taxontitle">
		<xs:annotation>
			<xs:documentation>'title' of taxon (e.g. family name, species, etc.)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="taxontitleType">
					<xs:attribute name="num" type="xs:string" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="biography" type="biographyType">
		<xs:annotation>
			<xs:documentation>biography text string</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datepub">
		<xs:annotation>
			<xs:documentation>dates of publication</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addenda" type="addendaType">
		<xs:annotation>
			<xs:documentation>addenda, corrections, etc.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendum" type="addendumType">
		<xs:annotation>
			<xs:documentation>one addendum</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumFamily">
		<xs:annotation>
			<xs:documentation>to which family does the addendum belong?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumTaxon">
		<xs:annotation>
			<xs:documentation>to what taxon is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumVolume">
		<xs:annotation>
			<xs:documentation>to what volume is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumPage">
		<xs:annotation>
			<xs:documentation>to what page is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumCollector">
		<xs:annotation>
			<xs:documentation>to what collector('s biography) is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="taxon">
		<xs:annotation>
			<xs:documentation>taxon</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0"/>
				<xs:element ref="taxontitle">
					<xs:annotation>
						<xs:documentation>'title' of taxon treatment (e.g. family name, species, etc.)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="writer" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="textSection" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="key" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="nomenclature" maxOccurs="unbounded"/>
				<xs:element ref="feature" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="notes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figure" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnote" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="class" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="excluded"/>
						<xs:enumeration value="dubious"/>
						<xs:enumeration value="cultivated"/>
						<xs:enumeration value="insufficient"/>
						<xs:enumeration value="expected"/>
						<xs:enumeration value="new"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="nomenclature">
		<xs:annotation>
			<xs:documentation>nomenclature</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="homotypes" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="homotypes">
		<xs:annotation>
			<xs:documentation>homotypes</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="nom" maxOccurs="unbounded"/>
				<xs:element ref="nameType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="specimenType" minOccurs="0"/>
				<xs:element ref="notes" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="key" type="keyType">
		<xs:annotation>
			<xs:documentation>key</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="keyTitle">
		<xs:annotation>
			<xs:documentation>key title</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="couplet">
		<xs:annotation>
			<xs:documentation>couplet</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="question" maxOccurs="unbounded"/>
				<xs:element ref="keynotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="num" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="question">
		<xs:annotation>
			<xs:documentation>question</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:element ref="couplet" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="toCouplet" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="toTaxon" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="toKey" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="keynotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="num" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="num">
		<xs:annotation>
			<xs:documentation>number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="text">
		<xs:annotation>
			<xs:documentation>question text</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="footnote" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="annotation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="toCouplet">
		<xs:annotation>
			<xs:documentation>goto couplet</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="num" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="toTaxon">
		<xs:annotation>
			<xs:documentation>goto taxon</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="num" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="toKey">
		<xs:annotation>
			<xs:documentation>goto key</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0">
				<xs:element ref="toCouplet" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="toTaxon" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="num" type="xs:string" use="optional"/>
			<xs:attribute name="ref" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="keynotes">
		<xs:annotation>
			<xs:documentation>key-related notes</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="footnote" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="annotation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="figure" type="figureType">
		<xs:annotation>
			<xs:documentation>figure</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="figureTitle">
		<xs:annotation>
			<xs:documentation>figure title</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="url">
		<xs:annotation>
			<xs:documentation>figure location URL</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="figureLegend">
		<xs:annotation>
			<xs:documentation>figure legend</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="collector" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fieldNum" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fullName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnote" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="annotation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figure" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="legendString">
		<xs:annotation>
			<xs:documentation>legend string</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="collector" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fieldNum" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnote" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="annotation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="footnote" type="footnoteType">
		<xs:annotation>
			<xs:documentation>footnote</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="notes" type="notesType">
		<xs:annotation>
			<xs:documentation>notes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="footnoteString" type="footnoteStringType">
		<xs:annotation>
			<xs:documentation>footnote string</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="lifeCyclePeriods">
		<xs:annotation>
			<xs:documentation>important periods in lifecycle</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice>
				<xs:element ref="dates"/>
			</xs:choice>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="flowering"/>
						<xs:enumeration value="fruiting"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="habitat">
		<xs:annotation>
			<xs:documentation>habitat</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="altitude" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="lifeCyclePeriods" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="habitatList" type="habitatListType">
		<xs:annotation>
			<xs:documentation>list of habitats</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="altitude">
		<xs:annotation>
			<xs:documentation>altitude</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="distributionList" type="distributionListType">
		<xs:annotation>
			<xs:documentation>list of locations where a species occurs</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="distributionLocality">
		<xs:annotation>
			<xs:documentation>list item (locality) for distribution</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="coordinates" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="world"/>
						<xs:enumeration value="country"/>
						<xs:enumeration value="continent"/>
						<xs:enumeration value="province"/>
						<xs:enumeration value="region"/>
						<xs:enumeration value="district"/>
						<xs:enumeration value="locality"/>
						<xs:enumeration value="county"/>
						<xs:enumeration value="state"/>
						<xs:enumeration value="other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="collection" type="collectionType">
		<xs:annotation>
			<xs:documentation>collection</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="collector">
		<xs:annotation>
			<xs:documentation>collector</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fieldNum">
		<xs:annotation>
			<xs:documentation>field number</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence minOccurs="0">
				<xs:element ref="annotation" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="references" type="referencesType">
		<xs:annotation>
			<xs:documentation>references</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="reference" type="referenceType">
		<xs:annotation>
			<xs:documentation>individual reference</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="citation" type="citationType">
		<xs:annotation>
			<xs:documentation>citation (in nomenclature)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="refNum">
		<xs:annotation>
			<xs:documentation>reference number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="refPart" type="refPartType">
		<xs:annotation>
			<xs:documentation>reference atomization</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dates" type="datesType">
		<xs:annotation>
			<xs:documentation>for dates</xs:documentation>
		</xs:annotation>
	</xs:element>

					<xs:annotation>
						<xs:documentation>name parts, citations, homonyms, and basionyms, also author comments</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="subChar" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>subcharacter</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="childChar" minOccurs="0"/>

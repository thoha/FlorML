			<xs:element ref="fullName" minOccurs="0"/>
			<xs:element ref="locality" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="dates" type="datesType" minOccurs="0"/>
			<xs:element name="string" type="stringType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="collectionStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>deprecated, use collectionAndType instead. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="collectionTypeStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>deprecated, use collectionAndType instead.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="alternativeCollectionTypeStatus" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>deprecated, use collectionAndType instead.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="subCollection" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>subcollection, e.g. flowers, fruits, buds, leaves</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subGathering" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>e.g. flowers, fruits, buds, leaves, often followed by a date. Deprecated, use subCollection instead and atomize the date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeNotes" type="notesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Notes on the type or the type status. DON'T put nomenclatural notes in here!</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="gatheringNotes" minOccurs="0"/>
			<xs:element name="unhandled" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>for text in atomised data for which it is doubtful how it has to be handled - use when you are intentionally not marking up a piece of text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0"/>
			<xs:element name="references" type="referencesType" minOccurs="0"/>
			<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
